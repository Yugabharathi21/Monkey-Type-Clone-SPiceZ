{
  "version": 2,
  "name": "typemonkey-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "@vite_api_base_url",
    "VITE_APP_NAME": "@vite_app_name",
    "VITE_APP_VERSION": "@vite_app_version",
    "VITE_NODE_ENV": "production"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "@vite_api_base_url",
      "VITE_APP_NAME": "@vite_app_name", 
      "VITE_APP_VERSION": "@vite_app_version",
      "VITE_NODE_ENV": "production"
    }
  }
}
